{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "tsc --build apps/api/tsconfig.app.json",
            "tsc-alias -p apps/api/tsconfig.app.json"
          ],
          "parallel": false
        },
        "outputs": [
          "{workspaceRoot}/dist/apps/api/src"
        ]
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "api:build",
          "inspect": true,
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "api:build:development"
          },
          "production": {
            "buildTarget": "api:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "@fastify/static": "^8.3.0",
    "@fastify/view": "^11.1.1",
    "@knaadh/nestjs-drizzle-pg": "^1.2.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@node-rs/argon2": "^2.0.2",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "fastify": "5.6.2",
    "ioredis": "^5.8.2",
    "nestjs-zod": "^5.0.1",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "poolifier": "^5.1.6",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/nodemailer": "^7.0.4",
    "drizzle-kit": "^0.31.6",
    "node-loader": "^2.1.0",
    "tsc-alias": "^1.8.16",
    "typescript": "~5.8.3"
  }
}
