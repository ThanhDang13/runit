{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "--node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "--node-env=development"
            ]
          }
        }
      },
      "build-worker": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack --config apps/api/webpack.worker.config.js --mode production"
        },
        "configurations": {
          "development": {
            "command": "webpack --config apps/api/webpack.worker.config.js --mode development"
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build",
          "build-worker"
        ],
        "options": {
          "buildTarget": "api:build",
          "inspect": true,
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "api:build:development"
          },
          "production": {
            "buildTarget": "api:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "@fastify/static": "^8.3.0",
    "@fastify/view": "^11.1.1",
    "@knaadh/nestjs-drizzle-pg": "^1.2.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@node-rs/argon2": "^2.0.2",
    "axios": "^1.13.2",
    "drizzle-orm": "^0.44.7",
    "nestjs-zod": "^5.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "poolifier": "^5.1.6",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.6",
    "node-loader": "^2.1.0"
  }
}
